{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "esmodules": true
        },
        "useBuiltIns": "usage",
        "corejs": {
          "version": "3.37",
          "proposals": true
        },
        "bugfixes": true
      }
    ],
    "next/babel"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-react-jsx",
      {
        "runtime": "automatic",
        "importSource": "@wearearchangel/handcrafted-plugin-template-syntax-react",
        "throwIfNamespace": false
      }
    ]
  ]
}
